{
  "name": "@armco/armcostatic",
  "version": "1.0.0",
  "description": "Server for static content across Armco",
  "author": "mohit.nagar@armco.tech",
  "license": "ISC",
  "scripts": {
    "lint": "eslint --ext .ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "APP_PORT=5003 NODE_ENV=development nodemon ./src/index.ts -e js,ts,json",
    "start": "node ./lib/index.js",
    "font-summary": "node src/fonts/generate-api-response.js",
    "font-description": "node src/fonts/generateFontDescriptor.js",
    "font-gen-summary": "npm run font-summary && src/fonts/goofoffline",
    "font-gen": "src/fonts/goofoffline"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@armco/node-starter-kit": "^1.3.5",
    "@fontsource/open-sans": "^4.5.2",
    "@fontsource/roboto": "^4.5.1",
    "axios": "^1.4.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^5.2.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "multer": "^1.4.5-lts.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "sharp": "^0.32.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.10",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^0.2.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.4"
  },
  "private": true
}
